{% extends 'base.html.twig' %}

{% block content %}
    <h1>Prestamo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prestamo.id }}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{% if prestamo.fechaInicio %}{{ prestamo.fechaInicio|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechafin</th>
                <td>{% if prestamo.fechaFin %}{{ prestamo.fechaFin|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Atendido</th>
                <td>{% if prestamo.atendido %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Ubicacion</th>
                <td>{{ prestamo.ubicacion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('prestamos_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('prestamos_edit', { 'id': prestamo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
